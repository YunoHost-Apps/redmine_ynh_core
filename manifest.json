{
    "name": "Redmine",
    "id": "redmine",
    "packaging_format": 1,
    "description": {
        "en": "Redmine is a flexible project management web application.",
        "fr": "Redmine est une application web flexible de gestion de projet."
    },
    "version": "3.3.0",
    "url": "https://www.redmine.org/",
    "license": "GPL-2.0",
    "maintainer": {
		"name": "Maniack Crudelis",
		"email": "maniackc_dev@crudelis.fr"
    },
    "requirements": {
        "yunohost": ">= 2.7.2"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name for redmine",
                    "fr": "Choisissez un nom de domaine pour redmine"
                },
                "example": "example.com"
            }
        ]
    }
}
